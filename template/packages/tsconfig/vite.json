{
    "$schema": "https://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "allowJs": false,
        "esModuleInterop": false,
        "isolatedModules": true,
        "jsx": "react-jsx",
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "module": "ESNext",
        "moduleResolution": "bundler",
        "noEmit": true,
        "target": "ESNext",
        "types": ["vite/client"],
        "resolveJsonModule": true
    }
}
