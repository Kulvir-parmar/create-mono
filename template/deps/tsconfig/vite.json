{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "Display": "Vite",
    "compilerOptions": {
        "allowJs": false,
        "esModuleInterop": false,
        "jsx": "react-jsx",
        "lib": ["dom", "dom.iterable", "esnext"],
        "module": "ESNext",
        "moduleDetection": "force",
        "moduleResolution": "bundler",
        "noEmit": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "target": "ESNext",
        "types": ["vite/client"]
    }
}
