{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "compilerOptions": {
        "plugins": [
            {
                "name": "next"
            }
        ],
        "incremental": true,
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowJs": true,
        "jsx": "preserve",
        "noEmit": true,
        "resolveJsonModule": true
    }
}
